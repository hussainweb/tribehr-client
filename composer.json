{
    "name": "hussainweb/tribehr-client",
    "description": "TribeHR client is a Guzzle based client to interact with TribeHR API.",
    "type": "library",
    "keywords": [
        "tribehr",
        "tribehr api",
        "kudos"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Hussain Abbas",
            "email": "hussainweb@gmail.com",
            "homepage": "http://hussainweb.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "lib-libxml": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Hussainweb\\TribeHr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hussainweb\\TribeHr\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    }
}
